{"ast":null,"code":"import WordForm from \"./components/WordForm.vue\";\nimport WordList from \"./components/WordList.vue\";\nimport DefButton from \"./components/UI/DefButton.vue\";\nexport default {\n  components: {\n    WordForm,\n    WordList,\n    DefButton\n  },\n\n  data() {\n    return {\n      words: [],\n      dialogVisible: false,\n      isWordsLoading: true\n    };\n  },\n\n  methods: {\n    addCouple(couple) {\n      this.words.push(couple);\n      this.dialogVisible = false;\n    },\n\n    removeWord(word) {\n      this.words = this.words.filter(p => p.id !== word.id);\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async fetchWords() {\n      try {\n        fetch(\"https://my-json-server.typicode.com/WKlon/Worddb/words\").then(response => response.json()).then(json => {\n          this.words = json;\n        });\n        this.isWordsLoading = true;\n      } catch (e) {\n        alert(\"Loading ERROR\");\n      } finally {}\n    }\n\n  },\n\n  mounted() {\n    this.fetchWords();\n  }\n\n};","map":{"version":3,"mappings":"AAkBC,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,SAAP,MAAsB,+BAAtB;AACA,eAAe;EACdC,UAAU,EAAE;IACXH,QADW;IAEXC,QAFW;IAGXC;EAHW,CADE;;EAMdE,IAAI,GAAG;IACN,OAAO;MACNC,KAAK,EAAE,EADD;MAENC,aAAa,EAAE,KAFT;MAGNC,cAAc,EAAE;IAHV,CAAP;EAKA,CAZa;;EAadC,OAAO,EAAE;IACRC,SAAS,CAACC,MAAD,EAAS;MACjB,KAAKL,KAAL,CAAWM,IAAX,CAAgBD,MAAhB;MACA,KAAKJ,aAAL,GAAqB,KAArB;IACA,CAJO;;IAKRM,UAAU,CAACC,IAAD,EAAO;MAChB,KAAKR,KAAL,GAAa,KAAKA,KAAL,CAAWS,MAAX,CAAmBC,CAAD,IAAOA,CAAC,CAACC,EAAF,KAASH,IAAI,CAACG,EAAvC,CAAb;IACA,CAPO;;IAQRC,UAAU,GAAG;MACZ,KAAKX,aAAL,GAAqB,IAArB;IACA,CAVO;;IAWR,MAAMY,UAAN,GAAmB;MAClB,IAAI;QACHC,KAAK,CAAC,wDAAD,CAAL,CACEC,IADF,CACQC,QAAD,IAAcA,QAAQ,CAACC,IAAT,EADrB,EAEEF,IAFF,CAEQE,IAAD,IAAU;UACf,KAAKjB,KAAL,GAAaiB,IAAb;QACA,CAJF;QAKA,KAAKf,cAAL,GAAsB,IAAtB;MACD,CAPA,CAOE,OAAOgB,CAAP,EAAU;QACXC,KAAK,CAAC,eAAD,CAAL;MACD,CATA,SASU,CACV;IACA;;EAvBO,CAbK;;EAsCdC,OAAO,GAAG;IACT,KAAKP,UAAL;EACA;