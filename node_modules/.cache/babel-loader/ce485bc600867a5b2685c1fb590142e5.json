{"ast":null,"code":"import WordForm from \"./components/WordForm.vue\";\nimport WordList from \"./components/WordList.vue\";\nimport DefButton from \"./components/UI/DefButton.vue\";\nexport default {\n  components: {\n    WordForm,\n    WordList,\n    DefButton\n  },\n\n  data() {\n    return {\n      words: [],\n      dialogVisible: false\n    };\n  },\n\n  methods: {\n    addCouple(couple) {\n      this.words.push(couple);\n      this.dialogVisible = false;\n    },\n\n    removeWord(word) {\n      this.words = this.words.filter(p => p.id !== word.id);\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async fetchWords() {\n      fetch(\"https://my-json-server.typicode.com/WKlon/Worddb/words\").then(response => response.json()).then(json => {\n        this.words = json;\n        console.log(this.words);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.fetchWords();\n  }\n\n};","map":{"version":3,"mappings":"AAaC,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,SAAP,MAAsB,+BAAtB;AACA,eAAe;EACdC,UAAU,EAAE;IACXH,QADW;IAEXC,QAFW;IAGXC;EAHW,CADE;;EAMdE,IAAI,GAAG;IACN,OAAO;MACNC,KAAK,EAAE,EADD;MAENC,aAAa,EAAE;IAFT,CAAP;EAIA,CAXa;;EAYdC,OAAO,EAAE;IACRC,SAAS,CAACC,MAAD,EAAS;MACjB,KAAKJ,KAAL,CAAWK,IAAX,CAAgBD,MAAhB;MACA,KAAKH,aAAL,GAAqB,KAArB;IACA,CAJO;;IAKRK,UAAU,CAACC,IAAD,EAAO;MAChB,KAAKP,KAAL,GAAa,KAAKA,KAAL,CAAWQ,MAAX,CAAmBC,CAAD,IAAOA,CAAC,CAACC,EAAF,KAASH,IAAI,CAACG,EAAvC,CAAb;IACA,CAPO;;IAQRC,UAAU,GAAG;MACZ,KAAKV,aAAL,GAAqB,IAArB;IACA,CAVO;;IAWR,MAAMW,UAAN,GAAmB;MAClBC,KAAK,CAAC,wDAAD,CAAL,CACEC,IADF,CACQC,QAAD,IAAcA,QAAQ,CAACC,IAAT,EADrB,EAEEF,IAFF,CAEQE,IAAD,IAAU;QACf,KAAKhB,KAAL,GAAagB,IAAb;QACAC,OAAO,CAACC,GAAR,CAAY,KAAKlB,KAAjB;MACA,CALF;IAMA;;EAlBO,CAZK;;EAgCdmB,OAAO,GAAG;IACT,KAAKP,UAAL;EACA;;AAlCa,CAAf","names":["WordForm","WordList","DefButton","components","data","words","dialogVisible","methods","addCouple","couple","push","removeWord","word","filter","p","id","showDialog","fetchWords","fetch","then","respon